{
  "name": "@aytordev/readme-generator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "build": "tsx src/index.ts",
    "build:local": "set -a; [ -f .env ] && . ./.env; set +a; tsx src/index.ts",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check --write .",
    "test": "vitest"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@octokit/graphql": "^9.0.3",
    "@octokit/rest": "^22.0.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  }
}
